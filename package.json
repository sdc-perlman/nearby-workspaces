    "test-server": "jest server/spec --detectOpenHandles --forceExit",
    "app-stress-get": "artillery run stress-test/app-stress-get.yml",
    "app-stress-post": "artillery run stress-test/app-stress-post.yml",
    "proxy-stress-get": "artillery run stress-test/proxy-stress-get.yml",
    "proxy-stress-post": "artillery run stress-test/proxy-stress-post.yml",
    "warmup-cache": "artillery run stress-test/warmup-cache.yml"
  },
  "dependencies": {
    "@babel/core": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "aws-sdk": "^2.839.0",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "csv-writer": "^1.6.0",
    "del": "^6.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "faker": "^5.5.2",
    "mongoose": "^5.11.11",
    "morgan": "^1.10.0",
    "nano": "^9.0.3",
    "newrelic": "^7.2.0",
    "node-couchdb": "^1.3.0",
